<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.ucai.fulicenter">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".application.FuLiCenterApplication"
        android:allowBackup="true"
        android:icon="@mipmap/launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.MainActivity" />
        <activity android:name=".ui.activity.SPActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.BoutiqueActivity" />
        <activity android:name=".ui.activity.Goods2Activity" />
        <activity
            android:name=".ui.activity.Categroy_Activity"
            android:launchMode="standard" />
        <activity android:name=".ui.activity.LoginActivity" />
        <activity android:name=".ui.activity.RegistrActivity" />
        <activity android:name=".ui.activity.SetUserActivity" />
        <activity android:name=".ui.activity.UpNickActivity" />
        <activity android:name=".ui.utils.ClipImageActivity" />
        <activity android:name=".ui.activity.CollectActivity" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".ui.activity.OrderActivity"></activity>
    </application>

</manifest>