<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="cn.ucai.fulicenter.ui.fiagment.CanterFragment">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="vertical"
        android:background="#e9fc430b">
      <TextView
          android:id="@+id/tvSetCenter"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="设置"
          android:textColor="#fff"
          android:textSize="20dp"
          android:padding="15dp"
          android:layout_alignParentRight="true"
          />
        <TextView
            android:id="@+id/tv"
            android:layout_width="20dp"
            android:layout_height="match_parent" />
        <ImageView
            android:id="@+id/ivAvatares"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_toRightOf="@id/tv"
            android:layout_centerVertical="true"
            android:src="@drawable/selector_menu_item_personal_center"/>
        <TextView
            android:id="@+id/tv1"
            android:layout_toRightOf="@id/ivAvatares"
            android:layout_width="20dp"
            android:layout_height="match_parent" />
        <TextView
            android:id="@+id/tvNick"
            android:layout_toRightOf="@id/tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="19dp"
            android:textColor="#fff"
            android:text="昵称"
            android:layout_centerVertical="true"/>
        <ImageView
            android:id="@+id/ivEVM"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/tvNick"
            android:layout_centerVertical="true"
            android:src="@drawable/tb_icon_home_qrcode_gray"
            />

    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:background="#f0e23500">
        <RelativeLayout
            android:gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tvhint"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10"
                android:textColor="#fff"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="收藏的宝贝"
                android:paddingTop="10dp"
                android:textSize="16dp"
                android:textColor="#fff"
                android:layout_below="@id/tvhint"
                android:layout_centerHorizontal="true"/>

        </RelativeLayout>
        <TextView
            android:layout_width="3dp"
            android:layout_height="40dp"
            android:background="#595d5a5a"
            android:layout_gravity="center_vertical"/>
        <RelativeLayout
            android:gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tvhint1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10"
                android:textColor="#fff"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="收藏的店铺"
                android:paddingTop="10dp"
                android:textSize="16dp"
                android:textColor="#fff"
                android:layout_below="@id/tvhint1"
                android:layout_centerHorizontal="true"/>

        </RelativeLayout>
        <TextView
            android:layout_width="3dp"
            android:layout_height="40dp"
            android:background="#595d5a5a"
            android:layout_gravity="center_vertical"/>
        <RelativeLayout
            android:gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/tvhint2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10"
                android:textColor="#fff"
                android:layout_centerHorizontal="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的足迹"
                android:paddingTop="10dp"
                android:textSize="16dp"
                android:textColor="#fff"
                android:layout_below="@id/tvhint2"
                android:layout_centerHorizontal="true"/>

        </RelativeLayout>

    </LinearLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:layout_centerVertical="true"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:textSize="19dp"
                    android:textColor="#000"
                    android:text="我的订单"
                    android:gravity="center_vertical"/>
                <ImageView
                    android:layout_centerVertical="true"
                    android:id="@id/iv"
                    android:paddingRight="10dp"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/right_arrow_icon"/>
                <TextView
                    android:id="@id/tv"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@id/iv"
                    android:paddingRight="6dp"
                    android:text="查看已购全部订单"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </RelativeLayout>
            <LinearLayout
                android:background="#fff"
                android:paddingTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/ivOrder1"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:src="@drawable/order_list1"/>
                <ImageView
                    android:id="@+id/ivOrder2"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:src="@drawable/order_list2"/>
                <ImageView
                    android:id="@+id/ivOrder3"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:src="@drawable/order_list3"/>
                <ImageView
                    android:id="@+id/ivOrder4"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:src="@drawable/order_list4"/>
                <ImageView
                    android:id="@+id/ivOrder5"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:src="@drawable/order_list5"/>

            </LinearLayout>
            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:layout_centerVertical="true"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:textSize="19dp"
                    android:textColor="#000"
                    android:text="我的卡卷包"
                    android:gravity="center_vertical"/>
                <ImageView
                    android:layout_centerVertical="true"
                    android:id="@id/iv"
                    android:paddingRight="10dp"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/right_arrow_icon"/>
                <TextView
                    android:id="@id/tv"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@id/iv"
                    android:paddingRight="6dp"
                    android:text=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </RelativeLayout>
            <LinearLayout
                android:paddingTop="5dp"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="60dp">
                <ImageView

                    android:src="@drawable/center_card_coupon1"
                    android:layout_width="50dp"
                    android:layout_height="match_parent" />
                <RelativeLayout

                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_centerVertical="true"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:textSize="16dp"
                        android:textColor="#ca1e1e1e"
                        android:text="生活卷"
                        android:gravity="center_vertical"/>
                    <ImageView
                        android:id="@id/iv"
                        android:paddingRight="10dp"
                        android:layout_alignParentRight="true"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/right_arrow_icon"/>
                    <TextView
                        android:id="@id/tv"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@id/iv"
                        android:paddingRight="6dp"
                        android:text=""
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>

            </LinearLayout>
            <LinearLayout
                android:paddingTop="5dp"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="60dp">
                <ImageView

                    android:src="@drawable/center_card_coupon2"
                    android:layout_width="50dp"
                    android:layout_height="match_parent" />
                <RelativeLayout

                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_centerVertical="true"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:textSize="16dp"
                        android:textColor="#ca1e1e1e"
                        android:text="网店卷"
                        android:gravity="center_vertical"/>
                    <ImageView
                        android:id="@id/iv"
                        android:paddingRight="10dp"
                        android:layout_alignParentRight="true"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/right_arrow_icon"/>
                    <TextView
                        android:id="@id/tv"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@id/iv"
                        android:paddingRight="6dp"
                        android:text="暂无可用优惠卷"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RelativeLayout>

            </LinearLayout>
            <LinearLayout
                android:paddingTop="5dp"
                android:gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="60dp">
                <ImageView

                    android:src="@drawable/center_card_coupon3"
                    android:layout_width="50dp"
                    android:layout_height="match_parent" />
                <RelativeLayout

                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@id/iv"
                        android:paddingRight="10dp"
                        android:layout_alignParentRight="true"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@drawable/right_arrow_icon"/>
                    <TextView
                        android:id="@id/tv"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@id/iv"
                        android:paddingRight="6dp"
                        android:text="一卡在手，逛店无忧"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:textSize="16dp"
                        android:textColor="#ca1e1e1e"
                        android:text="会员卡"
                        android:gravity="center_vertical"
                        android:layout_centerVertical="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/textView" />
                </RelativeLayout>


            </LinearLayout>
            <RelativeLayout

                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:layout_centerVertical="true"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:textSize="19dp"
                    android:textColor="#000"
                    android:text="我的专享特权"
                    android:gravity="center_vertical"/>
                <ImageView
                    android:layout_centerVertical="true"
                    android:id="@id/iv"
                    android:paddingRight="10dp"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/right_arrow_icon"/>
                <TextView
                    android:id="@id/tv"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@id/iv"
                    android:paddingRight="6dp"
                    android:text="领金币，看优惠"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </RelativeLayout>







        </LinearLayout>
    </ScrollView>



</LinearLayout>
